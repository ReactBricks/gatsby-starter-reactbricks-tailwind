{
  "name": "gatsby-starter-reactbricks",
  "private": true,
  "description": "A simple starter to get started quickly with React Bricks and Gatsby",
  "version": "1.3.5",
  "author": "Matteo Frana <f2@f2.net>",
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "bluebird": "^3.7.2",
    "gatsby": "^4.5.2",
    "gatsby-plugin-manifest": "^4.5.1",
    "gatsby-plugin-offline": "^5.5.1",
    "gatsby-plugin-postcss": "^5.5.0",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.26.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bricks": "^3.2.8",
    "react-bricks-ui": "^0.3.20",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.5",
    "concurrently": "^7.0.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.13"
  },
  "keywords": [
    "gatsby",
    "react-bricks",
    "react bricks",
    "tailwind"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && gatsby build",
    "build:css": "tailwindcss -o ./src/css/tailwind.css",
    "develop": "concurrently \"npm run develop:css\" \"gatsby develop\"",
    "develop:css": "tailwindcss -o ./src/css/tailwind.css --watch",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactbricks/gatsby-starter-reactbricks"
  },
  "bugs": {
    "url": "https://github.com/reactbricks/gatsby-starter-reactbricks/issues"
  }
}
